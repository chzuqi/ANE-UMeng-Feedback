package com.czqsoft.anes{	import flash.external.ExtensionContext;		/**	 * 友盟Feedback反馈系统 	 * 	 * @author czq	 * 	 */		public class UMFeedback	{				private static var _extContext:ExtensionContext;				private static var APPSTRING:String = 'com.czqsoft.umfeedback';				//与java端中Map里的key一致		private static const UM_FEEDBACK_INIT:String = "um_feedback_init";		private static const UM_FEEDBACK_OPEN:String = "um_feedback_open";		public function UMFeedback()		{					}				/**		 * 初始化友盟反馈系统 		 * 		 */				public static function init():void		{			_extContext=ExtensionContext.createExtensionContext(APPSTRING, null);			if (!_extContext)			{				trace("ERROR - Extension context is null. Please check if extension.xml is setup correctly.");			}else{				_extContext.call(UM_FEEDBACK_INIT);			}		}				/**		 * 打开友盟反馈面板 		 * 		 */				public static function open(appid:String = ''):void		{			if (_extContext){				_extContext.call(UM_FEEDBACK_OPEN, appid);			}		}								}}